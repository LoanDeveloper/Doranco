# -----------------------------------------------------
# 1 - Lecture des données et chargement des librairies
# -----------------------------------------------------
setwd("/mnt/data/Documents/Doranco/Statistiques et modèle SARIMAX/Jour 1")
df <- read.table("data_infarctus.csv", header = TRUE, sep = ";", dec = ".")
View(df)
head(df)
tail(df)
summary(df)
# ----------------------------
# 2 - Préparation des données
# ----------------------------
# 2.1 Variables quantitatives
age <- df$AGE
poids <- df$POIDS
taille <- df$TAILLE
# 2.2 Variables qualitatives
infarctus <- factor(df$INFARCT, levels = c(0,1), labels = c("non","oui"))
contraception <- factor(df$CO, levels = c(0,1), labels = c("non","oui"))
tabac <- factor(df$TABAC, levels = c(0,1,2), labels = c("non","actuelle", "ancienne"))
antecedant <- factor(df$ATCD, levels = c(0,1), labels = c("non","oui"))
hypertension <- factor(df$HTA, levels = c(0,1), labels = c("non","oui"))
# ----------------------------
# 3 - Analyse univariée
# ----------------------------
# 3.1 Variables quantitatives
hist(age)
summary(age)
sd(age)
boxplot(age)
hist(taille)
summary(taille)
sd(taille)
boxplot(taille)
hist(poids)
summary(poids)
sd(poids, na.rm = TRUE)
boxplot(poids)
# 3.2 Variables qualitatives
table(infarctus)
table(contraception)
table(tabac)
table(antecedant)
table(hypertension)
